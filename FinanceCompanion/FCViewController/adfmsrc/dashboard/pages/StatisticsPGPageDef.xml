<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.23.11" id="StatisticsPGPageDef"
                Package="dashboard.pages">
  <parameters/>
  <executables>
    <variableIterator id="variables">
      <variable Name="entityNameVar" Type="java.lang.String"/>
    </variableIterator>
    <iterator Binds="root" RangeSize="25" DataControl="EntityDC" id="EntityDCIterator"/>
    <accessorIterator MasterBinding="EntityDCIterator" Binds="entity" RangeSize="-1" DataControl="EntityDC"
                      BeanClass="com.fincomp.mobile.pojo.lov.EntityBO" id="entityIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="DashboardDC" id="DashboardDCIterator"/>
    <accessorIterator MasterBinding="DashboardDCIterator" Binds="dashboardDetails" RangeSize="-1"
                      DataControl="DashboardDC" BeanClass="com.fincomp.mobile.pojo.dashboard.DashboardBO"
                      id="dashboardDetailsIterator"/>
    <accessorIterator MasterBinding="DashboardDCIterator" Binds="issueDetails" RangeSize="25" DataControl="DashboardDC"
                      BeanClass="com.fincomp.mobile.pojo.dashboard.IssueDetailsBO" id="issueDetailsIterator"/>
  </executables>
  <bindings>
    <methodAction id="gotoSpringboard" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoSpringboard"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider"/>
    <methodAction id="gotoFeature" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoFeature"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider">
      <NamedData NDName="featureId" NDType="java.lang.String"/>
    </methodAction>
    <list IterBinding="variables" id="name" DTSupportsMRU="true" SelectItemValueMode="ListObject"
          StaticList="false" ListIter="entityIterator" NullValueFlag="start" NullValueId="name_null">
      <AttrNames>
        <Item Value="entityNameVar"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="entityName"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="entityName"/>
      </ListDisplayAttrNames>
    </list>
    <tree IterBinding="dashboardDetailsIterator" id="dashboardDetails">
      <nodeDefinition DefName="com.fincomp.mobile.pojo.dashboard.DashboardBO" Name="dashboardDetails0">
        <AttrNames>
          <Item Value="issueCount"/>
          <Item Value="entity"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <tree IterBinding="issueDetailsIterator" id="issueDetails">
      <nodeDefinition DefName="com.fincomp.mobile.pojo.dashboard.IssueDetailsBO" Name="issueDetails0">
        <AttrNames>
          <Item Value="issueText"/>
          <Item Value="issueCount"/>
        </AttrNames>
      </nodeDefinition>
    </tree>

    <attributeValues IterBinding="dashboardDetailsIterator" id="owner">
      <AttrNames>
        <Item Value="owner"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="dashboardDetailsIterator" id="issueCount">
      <AttrNames>
        <Item Value="issueCount"/>
      </AttrNames>
    </attributeValues>
  </bindings>
  <ResourceBundle>
    <XliffBundle xmlns="http://xmlns.oracle.com/adfm/resourcebundle" id="com.fincomp.mobile.FCViewControllerBundle"/>
  </ResourceBundle>
</pageDefinition>
